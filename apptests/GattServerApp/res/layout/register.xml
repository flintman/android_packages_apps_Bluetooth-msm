<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <Button
        android:text="@string/Register_Value"
        android:id="@+id/button_register_app"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">
    </Button>
    <Button
        android:text="@string/Unregister_Value"
        android:id="@+id/button_unregister_app"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">
    </Button>
    <Button
        android:text="@string/Connect_Value"
        android:id="@+id/button_connect_app"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">
    </Button>
    <Button
        android:text="@string/Cancel_Connect_Req"
        android:id="@+id/button_cancel_connect_req"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">
    </Button>
    <Button
        android:text="@string/Disconnect_Value"
        android:id="@+id/button_disconnect_app"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">
    </Button>
    <Button
        android:text="@string/Add_To_Preferred_Device_List"
        android:id="@+id/button_add_to_preferred_device_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">
    </Button>
    <Button
        android:text="@string/Remove_From_Preferred_Device_List"
        android:id="@+id/button_remove_from_preferred_device_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">
    </Button>
    <Button
        android:text="@string/Clear_Preferred_Device_List"
        android:id="@+id/button_clear_preferred_device_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">
    </Button>
    <Button
        android:text="@string/Connect_Preferred_Device_List"
        android:id="@+id/button_connect_preferred_device_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">
    </Button>
    <Button
        android:text="@string/Cancel_Connect_Preferred_Device_List"
        android:id="@+id/button_cancel_connect_preferred_device_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">
    </Button>
    <Button
        android:text="@string/Auto_Connect"
        android:id="@+id/button_auto_connect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">
    </Button>
    <Button
        android:text="@string/Auto_Connect_Cancel"
        android:id="@+id/button_auto_connect_cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">
    </Button>
</LinearLayout>
